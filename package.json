{
  "name": "otedama",
  "version": "1.1.3",
  "description": "Otedama - Enterprise-Grade Professional Mining Platform with Zero-Knowledge Proof Authentication",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "start:ultra": "node index-ultra.js",
    "dev": "nodemon index.js --watch lib --watch config",
    "start:pool": "node start-mining-pool.js",
    "start:pool:cluster": "node start-mining-pool.js --mode cluster",
    "start:pool:federated": "node start-mining-pool.js --mode federated",
    "start:pool:enterprise": "node start-mining-pool.js --mode enterprise --regions us-east,us-west,eu-west",
    "start:miner": "node otedama-miner.js",
    "start:all": "concurrently \"npm run start:pool\" \"npm run start:miner\"",
    "start:profit-switching": "node start-mining-pool.js --enable-profit-switching",
    "start:monitoring": "node lib/monitoring/dashboard-server.js",
    "benchmark": "node scripts/benchmark-algorithms.js",
    "benchmark:performance": "node test/performance-benchmark.js",
    "benchmark:ultra": "node index-ultra.js --benchmark",
    "health": "node scripts/health-check.js",
    "health:full": "node scripts/comprehensive-health-check.js",
    "backup:now": "node scripts/backup-now.js",
    "backup:restore": "node scripts/restore-backup.js",
    "security:scan": "node scripts/security-scan.js",
    "security:audit": "node scripts/security-audit.js",
    "zkp:generate": "node scripts/generate-zkp-keys.js",
    "zkp:verify": "node scripts/verify-zkp-setup.js",
    "test": "jest --coverage --detectOpenHandles",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:unit": "jest test/unit --coverage",
    "test:integration": "jest test/integration --coverage",
    "test:system": "node test/system-test.js",
    "test:performance": "node test/performance-test.js",
    "test:security": "node test/security-test.js",
    "test:zkp": "node test/zkp-test.js",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:system",
    "lint": "eslint . --ext .js,.mjs --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .js,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write '**/*.{js,mjs,json,md}'",
    "format:check": "prettier --check '**/*.{js,mjs,json,md}'",
    "typecheck": "tsc --noEmit --allowJs --checkJs lib/**/*.js",
    "config:validate": "node scripts/validate-config.js",
    "config:generate": "node scripts/generate-config.js",
    "db:setup": "node scripts/setup-database.js",
    "db:migrate": "node scripts/migrate-database.js",
    "db:backup": "node scripts/backup-database.js",
    "db:restore": "node scripts/restore-database.js",
    "setup": "npm run config:generate && npm run db:setup && npm run zkp:generate",
    "deploy": "npm prune --production && npm run security:audit && npm run test:all",
    "deploy:docker": "docker build -t otedama:latest . && docker-compose up -d",
    "deploy:production": "node scripts/production-deploy.js",
    "docker:build": "docker build -t otedama:v1.1.0 .",
    "docker:run": "docker-compose up -d",
    "docker:logs": "docker-compose logs -f",
    "docker:stop": "docker-compose down",
    "docker:clean": "docker system prune -f",
    "monitoring:setup": "node scripts/setup-monitoring.js",
    "monitoring:dashboard": "node lib/monitoring/realtime-dashboard.js",
    "performance:optimize": "node scripts/optimize-performance.js",
    "performance:profile": "node --prof index.js",
    "performance:analyze": "node --prof-process isolate-*.log > profile.txt",
    "cleanup": "node scripts/cleanup-duplicates.js",
    "cleanup:logs": "node scripts/cleanup-logs.js",
    "validate": "node scripts/verify-structure.js",
    "validate:all": "npm run lint && npm run format:check && npm run config:validate && npm run validate",
    "docs:generate": "jsdoc -c jsdoc.conf.json",
    "docs:serve": "http-server docs -p 8080",
    "stats": "node scripts/pool-statistics.js",
    "maintenance": "node scripts/maintenance-mode.js",
    "version:patch": "npm version patch && git push --tags",
    "version:minor": "npm version minor && git push --tags",
    "version:major": "npm version major && git push --tags"
  },
  "keywords": [
    "mining",
    "cryptocurrency", 
    "blockchain",
    "p2p",
    "stratum-v2",
    "mining-pool",
    "bitcoin",
    "ethereum",
    "enterprise",
    "zkp",
    "zero-knowledge-proof",
    "high-performance",
    "national-scale",
    "security",
    "monitoring"
  ],
  "author": "Otedama Team",
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "^9.2.2",
    "express": "^4.18.2",
    "ws": "^8.14.2",
    "socket.io": "^4.5.4",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "commander": "^11.1.0",
    "prom-client": "^15.0.0",
    "bitcoinjs-lib": "^6.1.5",
    "ethers": "^6.9.0",
    "axios": "^1.6.2",
    "jsonwebtoken": "^9.0.2",
    "express-validator": "^7.0.1",
    "inquirer": "^9.2.12",
    "chalk": "^5.3.0",
    "ora": "^6.3.1",
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.11.0",
    "cli-table3": "^0.6.3",
    "ioredis": "^5.3.2",
    "rate-limiter-flexible": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "jsdoc": "^4.0.2",
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "babel-jest": "^29.7.0",
    "concurrently": "^8.2.0",
    "typescript": "^5.3.3",
    "http-server": "^14.1.1",
    "@types/jest": "^29.5.8",
    "@types/express": "^4.17.21",
    "@types/ws": "^8.5.10",
    "cross-env": "^7.0.3",
    "nyc": "^15.1.0",
    "c8": "^8.0.1",
    "autocannon": "^7.15.0",
    "clinic": "^13.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shizukutanaka/Otedama.git"
  },
  "bugs": {
    "url": "https://github.com/shizukutanaka/Otedama/issues"
  },
  "homepage": "https://github.com/shizukutanaka/Otedama"
}