  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "standalone": "node index.js --mode standalone",
    "pool": "node index.js --mode pool",
    "miner": "node index.js --mode miner",
    "gui": "electron lib/gui/miner-app.js",
    "build-gui": "electron-builder",
    "build:native": "cd native && node-gyp rebuild",
    "clean": "rm -rf node_modules native/build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint lib/**/*.js lib/**/*.ts",
    "lint:fix": "eslint lib/**/*.js lib/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "build:ts": "tsc",
    "dev:ts": "ts-node index.js",
    "setup": "node index.js --setup",
    "setup:wizard": "node index.js --wizard",
    "quick-start": "node quick-start.js",
    "health-check": "node scripts/health-check.js",
    "optimize-db": "node scripts/optimize-db.js",
    "benchmark": "node scripts/benchmark.js",
    "prepublishOnly": "npm test && npm run lint && npm run verify-fee-integrity",
    "preversion": "npm test",
    "postversion": "git push && git push --tags",
    "verify-fee-integrity": "node scripts/verify-fee-integrity.js",
    "prebuild": "npm run verify-fee-integrity"
  },
  "keywords": [
    "otedama",
    "p2p",
    "mining-pool",
